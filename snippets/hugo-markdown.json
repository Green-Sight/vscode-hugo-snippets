{
    "hugo-highlight": {
        "prefix": "h-highlight",
        "body": [
            "{{< highlight ${1|html,javascript,markdown,scss,typescript,terminal|} >}}",
            "$0",
            "{{< /highlight >}}"
        ],
        "description": "Highlight Shortcode"
    },
    "hugo-shortcode": {
        "prefix": "h-shortcode",
        "body": [
            "{{< ${1:shortcode} >}}"
        ],
        "description": "Shortcode Template"
    },
    "hugo-shortcode-param": {
        "prefix": "h-shortcode-param",
        "body": [
            "{{< ${1:shortcode} ${2:param} >}}"
        ],
        "description": "Shortcode Template w/ param"
    }
}